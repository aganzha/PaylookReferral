<html lang="ru-RU">
<!--<![endif]-->
<head>
    <meta charset="utf-8">
    <link rel="stylesheet" media="screen" href="@Url.Content("~/Content/screen.css")">
    <link href='http://fonts.googleapis.com/css?family=PT+Sans:400,700,400italic,700italic&subset=latin,cyrillic' rel='stylesheet' type='text/css'>
    <!--[if lt IE 9]><script src="js/html5shiv.js"></script><![endif]-->

    <script src="@Url.Content("~/Scripts/jquery-1.9.1.min.js")"></script>
    <script src="@Url.Content("~/Scripts/datepicker.js")"></script>
    <script src="@Url.Content("~/Scripts/common.js")"></script>

    <title>@ViewBag.Title</title>
</head>

<body>
    <!-- BEGIN .wrap -->
    <section class="wrap">

        <div class="main">
            <!-- BEGIN .header -->
            <header class="header">
                <div class="logo"><span>Кабинет партнера</span><i></i></div>
            </header>
            <!-- END .header -->
            <div class="title"><span>@ViewBag.Title</span></div>
            @RenderBody()
            <div class="push"></div>
        </div>
    </section>
    <!-- END .wrap -->
    @Html.Partial("Footer");

    <script type="text/javascript" src="/scripts/jquery.cookie.js"></script>
    <script type="text/javascript" src="/scripts/jquery.json-2.3.min.js"></script>
    <script type="text/javascript" src="/scripts/underscore-min.js"></script>
    
    @*<script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>*@

    <form action="" method="POST" id="buttonForm">
        <input type="hidden" name="returnUrl" value="@this.Request.Url.AbsoluteUri"/>
    </form>

    <script type="text/javascript">
        $(function () {

            $('button.go').click(function (e) {

                var elem = $(this);
                var url = elem.attr('href');
                document.location = url;

                return false;
            })
        });
    </script>

</body>
</html>

